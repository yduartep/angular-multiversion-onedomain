{
  "name": "angular-common-elements",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prestart:live": "npm run build",
    "start:live": "npx live-server dist",
    "postinstall": "node build.js",

    "build": "node build.js",
    "build:header": "ng build --project=header-demo-app --prod --output-hashing=none",
    "build:footer": "ng build --project=footer-demo-app --prod --output-hashing=none",
    "build:prod": "ng build --prod",

    "pack:footer:es2015": "jscat ./dist/footer-demo-app/runtime-es2015.js ./dist/footer-demo-app/polyfills-es2015.js ./dist/footer-demo-app/main-es2015.js > ./dist/es2015/footer-demo-element.js",
    "pack:footer:es5": "jscat ./dist/footer-demo-app/runtime-es5.js ./dist/footer-demo-app/polyfills-es5.js ./dist/footer-demo-app/main-es5.js > ./dist/es5/footer-demo-element.js",
    "pack:footer": "npm run pack:footer:es5 && npm run pack:footer:es2015",

    "pack:header:es2015": "jscat ./dist/header-demo-app/runtime-es2015.js ./dist/header-demo-app/polyfills-es2015.js ./dist/header-demo-app/main-es2015.js > ./dist/es2015/header-demo-element.js",
    "pack:header:es5": "jscat ./dist/header-demo-app/runtime-es5.js ./dist/header-demo-app/polyfills-es5.js ./dist/header-demo-app/main-es5.js > ./dist/es5/header-demo-element.js",
    "pack:header": "npm run pack:header:es5 && npm run pack:header:es2015",

    "clean:header": "rm -rf dist/header-demo-app",
    "clean:footer": "rm -rf dist/footer-demo-app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular-devkit/build-ng-packagr": "~0.803.29",
    "@angular/animations": "~8.2.14",
    "@angular/cli": "~8.3.20",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "document-register-element": "^1.7.2",
    "jscat": "0.0.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular-devkit/build-ng-packagr": "~0.803.29",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
